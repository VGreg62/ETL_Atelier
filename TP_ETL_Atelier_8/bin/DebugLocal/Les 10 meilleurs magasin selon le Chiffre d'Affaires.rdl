  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT        TOP (10) soh.SalesOrderID AS Store_Name, ter.Name AS Region, soh.SubTotal AS CA
FROM            Sales.SalesOrderHeader AS soh INNER JOIN
                         Sales.SalesTerritory AS ter ON ter.TerritoryID = soh.TerritoryID
ORDER BY CA DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Store_Name">
          <DataField>Store_Name</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CA">
          <DataField>CA</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
