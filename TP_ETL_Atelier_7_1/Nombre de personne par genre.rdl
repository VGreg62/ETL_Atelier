  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT        COUNT(*) AS Nb, E.Gender
FROM            Person.Person AS P INNER JOIN
                         HumanResources.Employee AS E ON P.BusinessEntityID = E.BusinessEntityID
WHERE        (E.Gender IN ('F', 'M'))
GROUP BY E.Gender</CommandText>
      </Query>
      <Fields>
        <Field Name="Nb">
          <DataField>Nb</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Gender">
          <DataField>Gender</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PersonneParTitre">
      <Query>
        <DataSourceName>AdventureWorks2017</DataSourceName>
        <CommandText>SELECT Count(*) As Nb, P.Title
  FROM [AdventureWorks2017].[Person].[Person] AS P
Group By P.Title</CommandText>
      </Query>
      <Fields>
